<!--logs.wxml-->
<view class="container lang-list">
  <view class="title">翻译成</view>
  <view class="item" hover-class="view-hover" data-chs="{{language.chs}}" data-lang="{{language.lang}}" data-index="{{index}}" wx:for="{{langList}}" wx:key="idex" wx:for-item="language" bindtap="onTapItem" >
    <view class="item-inner">
      <text class="txt">{{language.chs}}</text>
      <text class="iconfont icon-duihao" wx:if="{{index === curLang.index}}"></text>
    </view>
  </view>
</view>
