.container{
  position: relative;
}
.history-list {
  display: flex;
  flex-direction: column;
  padding: 40rpx;
}
.title {
  font-size: 26rpx;
  color: #8995a1;
}
.item {
  margin-top: 40rpx;
}

.item .query{
  color: #8995a1;
}
.item .result{
  margin-top: 16rpx;
}

.clear{
  position: fixed;
  top: 0 ;
  right: 0;
  font-size: 26rpx;
  color: #8995a1;
  padding: 40rpx;
}